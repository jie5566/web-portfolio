<div id="main">
  <div class="userContainer">
    <h1>Projects by Skill</h1>
    <h2>
    {{#if isLoggedIn}}
      Hi,
      {{name}}!
    {{/if}}
    </h2>
    <br>
    <div class="container">
        <table class="table table-striped table-hover">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Username</th>
                    <th>Password</th>
                    <th>isAdmin</th>
                </tr>
            </thead>
            <tbody>
                {{#each users}}
                <tr>
                    <td>{{uid}}</td>
                    <td>{{uname}}</td>
                    <td>{{username}}</td>
                    <td>{{password}}</td>
                    <td>{{isAdmin}}</td>
                    
                {{#if ../isAdmin}}
                <td>
                  <a href="/projects/update/{{uId}}">Modify</a>
                  <a href="/projects/delete/{{uId}}" class="important" >Delete</a>
                </td>
                {{/if}}
                </tr>
                {{/each}}
            </tbody>
        </table>

    </div>
  </div>

</div>